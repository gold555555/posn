#include <iostream>
using namespace std;

int main() {
    int n, m, i, j;
    cin >> n >> m;

    int arr[n][m];
    int rowSums[n];

    for (i = 0; i < n; i++) {
        for (j = 0; j < m; j++) {
            cin >> arr[i][j];
        }
    }

    for (i = 0; i < n; i++) {
        rowSums[i] = 0; 
        for (j = 0; j < m; j++) {
            rowSums[i] += arr[i][j]; 
        }
    }

    for (i = 0; i < n; i++) {
        cout  << rowSums[i] << endl;
    }
    
    int rowSum;
    for (i = 0; i < n; i++) {
        rowSums[i] = 0; 
        for (j = 0; j < m; j++) {
            rowSums[i] += arr[i][j]; 
        }
    }
    cout  << rowSums[i] << endl;
    
   /*     int colSums;
    for(j =0 ; j < m; j++){
        colSums[j]=0;
        for(i=0 ; i<n ; i++){
            colSums[j] += arr[i][j];
            }
     }
     for(j=0 ; j<m;j++){
        cout << colSums[j] << endl ;
    }*/
    return 0;
}

#include <iostream>
using namespace std;

int main() {
    int n, m, i, j;
    cin >> n >> m;

    int arr[n][m];
    int rowSums[n];
    int k;

    for (i = 0; i < n; i++) {
        for (j = 0; j < m; j++) {
            cin >> arr[i][j];
            
        }
    }
    cin >> k ;
for (i = 0; i < n; i++) {
        for (j = 0; j < m; j++) {
            if(k==arr[i][j]){
                cout << k << " ";
            }else {
                cout << "*" << " ";
            }
        }            cout << endl ;

    }
  

}


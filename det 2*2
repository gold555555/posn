#include <bits/stdc++.h>
using namespace std;

int main() {
    int n ,m;
    //cin >> n  ;
    int arr[2][2];
    for(int i=0; i<2 ; i++){
        for(int j=0 ; j<2 ; j++){
            cin >> arr[i][j];
        }
    }
    int x ;
    x = -(arr[0][1]*arr[1][0]) ;
    int y;
    y = arr[0][0]*arr[1][1] ;
    
    int det = 0;
    det = x+y ;
    cout << det << endl ;
}

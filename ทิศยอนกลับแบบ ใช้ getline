#include <bits/stdc++.h>
using namespace std;


int main() {
	string  input ;
	getline(cin,input);
	int length1 = input.length();
	//cout << length1;
	reverse(input.begin(),input.end());
	int n =length1;
	for(int i=0;i<n;i++){
		//cout << input[i];
		if(input[0] == 'N'|| input[0] == 'S'){
				if(input[i] =='N'){
						input[i] ='S';
				}else if(input[i] == 'S'){
						input[i] = 'N';
				}else if(input[i] == 'W'){
						input[i] = 'E';
				}else{
					input[i] = 'W';
						}
					cout << input[i];
		}else{
			return 0 ;
		}
	
	}
	
	
}
